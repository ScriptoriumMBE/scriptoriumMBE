{
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "devCommand": "npm run start",
    
    "routes": [
        {
        "src": "^/musica/?$",
        "headers": { "Location": "/music/" },
        "status": 301
        },
        { "handle": "filesystem" },
        { "src": "/(.*)", "status": 404, "dest": "/404" }
      ]

    
}
